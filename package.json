{
  "name": "webpack-babel-multi-target-plugin",
  "description": "A Webpack plugin that works with Babel to allow deployment of ES2015 builds targeted to modern browsers, with an ES5 fallback for legacy browsers.",
  "keywords": [
    "webpack-plugin",
    "babel",
    "babel-loader",
    "esnext",
    "esmodules",
    "es2016",
    "es6",
    "es7"
  ],
  "version": "0.2.0-alpha.13",
  "author": {
    "name": "Daniel Schaffer",
    "email": "dan@dandoes.net",
    "url": "https://github.com/DanielSchaffer"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:DanielSchaffer/webpack-babel-multi-target-plugin.git"
  },
  "homepage": "https://github.com/DanielSchaffer/webpack-babel-multi-target-plugin",
  "bugs": "https://github.com/DanielSchaffer/webpack-babel-multi-target-plugin/issues",
  "license": "ISC",
  "main": "index.js",
  "typings": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "src/*"
  ],
  "scripts": {
    "example": "ts-node examples/build",
    "examples": "ts-node examples/build",
    "start": "node examples/serve",
    "copy:package.json": "cp package.json ./dist",
    "copy:md": "cp *.md ./dist",
    "copy": "npm run copy:package.json && npm run copy:md",
    "prebuild": "rimraf dist",
    "build": "tsc -p ./tsconfig.build.json",
    "postbuild": "npm run copy",
    "postversion": "npm run build && cd ./dist && npm publish && cd ..",
    "postpublish": "git push && git push --tags",
    "precoverage": "rimraf coverage .nyc_output",
    "coverage": "nyc npm test",
    "test": "mocha src/**/*.spec.ts"
  },
  "peerDependencies": {
    "babel-loader": "^8.0.0-beta.0",
    "webpack": "^4.1.1"
  },
  "dependencies": {
    "lodash-es": "^4.17.5",
    "tapable": "^1.0.0",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@types/babel-core": "^6.25.3",
    "@types/core-js": "^0.9.46",
    "@types/fs-extra": "^5.0.1",
    "@types/html-webpack-plugin": "^2.30.2",
    "@types/jasmine": "^2.8.6",
    "@types/tapable": "^0.2.4",
    "@types/webpack": "^4.0.0",
    "@types/webpack-merge": "^4.1.2",
    "@types/webpack-sources": "^0.1.4",
    "awesome-typescript-loader": "^5.0.0-0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-commonjs-es2015-modules": "^4.0.1",
    "core-js": "^2.5.3",
    "file-loader": "^1.1.11",
    "fs-extra": "^5.0.0",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "mini-css-extract-plugin": "^0.2.0",
    "node-sass": "^4.7.2",
    "null-loader": "^0.1.1",
    "nyc": "^11.4.1",
    "pug-html-loader": "^1.1.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "to-string-loader": "^1.1.5",
    "ts-node": "^5.0.1",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.0"
  }
}
